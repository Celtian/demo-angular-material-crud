    "husky": "^9.0.11",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.5",
    "ngx-devkit-builders": "^1.3.1",
    "ngx-i18n-extract-regex-cli": "^0.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
