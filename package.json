  "private": true,
  "description": "Demo of angular material crud operations",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && yarn lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "postinstall": "ng run demo-angular-material-crud:version",
    "prepare": "husky",
    "i18n:extract-regex": "ngx-i18n-extract-regex-cli extract --library ngx-translate",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --format-indentation '  ' --string-as-default-value '███'",
    "fix-vulnerabilities": "npx yarn-audit-fix && npx browserslist --update-db"
  },
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/cdk": "^18.0.2",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/material": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "@gilsdav/ngx-translate-router": "^7.1.0",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "lodash-es": "^4.17.21",
